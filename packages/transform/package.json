{
  "name": "@glint/transform",
  "version": "0.9.1",
  "repository": "typed-ember/glint",
  "description": "Transforms Glimmer templates into TypeScript code that approximates their semantics",
  "license": "MIT",
  "author": "Dan Freeman (https://github.com/dfreeman)",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "README.md",
    "lib"
  ],
  "scripts": {
    "lint": "eslint . --max-warnings 0 && prettier --check src",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "build": "tsc --build",
    "prepack": "yarn build"
  },
  "dependencies": {
    "@glimmer/syntax": "^0.84.2",
    "@glint/config": "^0.9.1"
  },
  "devDependencies": {
    "@types/common-tags": "^1.8.0",
    "@types/debug": "^4.1.5",
    "common-tags": "^1.8.0",
    "vitest": "^0.18.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
